{
  "version": 2,
  "builds": [
    {
      "src": "Studynotion/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server2/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server2/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Studynotion/$1"
    }
  ]
} 